	<queryString language="hql">
		<![CDATA[select (
  select count(u.codi)
  from   es.caib.seycon.ng.model.UsuariEntity as u
  where  u.actiu='S'
) as users ,(
  select count(a.name)
  from   es.caib.seycon.ng.model.AccountEntity as a
  where  a.disabled = 0 and a.type != 'U'
) as resources,(
  select count(dis.codi)
  from   es.caib.seycon.ng.model.DispatcherEntity as dis
) as domains, (
  select count(gru.codi)
  from   es.caib.seycon.ng.model.GrupEntity as gru
) as ous, (
  select count(a.name)
  from   es.caib.seycon.ng.model.AccountEntity as a
  where  a.disabled = 0 and a.type != 'U'
) as accounts
from  es.caib.seycon.ng.model.ConfiguracioEntity as uu
where uu.codi='component.iam-core.version']]>
	</queryString>
